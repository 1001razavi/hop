  "scripts": {
    "start": "npm run tempfix && REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-app-rewired start",
    "dev": "BROWSER=none npm start",
    "clean": "rimraf build",
    "lint": "npx eslint --fix \"**/*.{ts,tsx}\"",
    "build": "npm run tempfix && REACT_APP_GIT_SHA=`git rev-parse --short HEAD` INLINE_RUNTIME_CHUNK=false react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "deploy": "npx --yes netlify-cli deploy --prod",
    "deploy:ipfs": "npx --yes ipfs-deploy -p infura build --no-open --no-clipboard",
    "deploy:local": "git stash && yarn clean && yarn build && yarn deploy",
    "tempfix": "./scripts/walletconnect_fix.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
